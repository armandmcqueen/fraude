{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    ".next/dev/types/**/*.ts",
    "**/*.mts",
    ".next/dev/dev/types/**/*.ts",
    "/var/folders/08/xmjh7n3s3rz3l8tyxgwv1r8m0000gn/T/fraude-test-next-1OW6kq/dev/types/**/*.ts",
    "/var/folders/08/xmjh7n3s3rz3l8tyxgwv1r8m0000gn/T/fraude-test-next-1OW6kq/dev/dev/types/**/*.ts",
    "/var/folders/08/xmjh7n3s3rz3l8tyxgwv1r8m0000gn/T/fraude-test-next-CUneYh/dev/types/**/*.ts",
    "/var/folders/08/xmjh7n3s3rz3l8tyxgwv1r8m0000gn/T/fraude-test-next-CUneYh/dev/dev/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
